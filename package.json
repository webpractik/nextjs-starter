{
    "name": "nextjs-starter",
    "version": "7.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "prepare": "node .husky/install.mjs",
        "build": "next build",
        "dev": "kill-port 3000 && next dev -p 3000",
        "prod": "next start",
        "check:ts": "tsc --project tsconfig.json --noEmit --pretty",
        "check:lint": "next lint",
        "check:format": "prettier . --write --cache --log-level=silent",
        "check:test": "vitest run",
        "check:e2e": "playwright test",
        "check:all": "npm run check:ts && npm run check:lint && npm run check:test && npm run check:format",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "analyze": "ANALYZE=true npm run build && statoscope validate --input .next/stats.json",
        "million": "MILLION=true npm run dev",
        "knip": "knip --strict",
        "jscpd": "npx jscpd",
        "clean": "rimraf .next",
        "dr:build": "npx declarative-routing build",
        "dr:build:watch": "npx declarative-routing build --watch",
        "storybook": "kill-port 6006 && storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "dependencies": {
        "@hookform/resolvers": "3.10.0",
        "@repo/api": "*",
        "@repo/core": "*",
        "@repo/design-tokens": "*",
        "@sentry/nextjs": "8.51.0",
        "@t3-oss/env-nextjs": "0.12.0",
        "@tanstack/react-query": "5.65.0",
        "@tanstack/react-query-devtools": "5.65.0",
        "@tanstack/react-query-next-experimental": "5.65.0",
        "@vercel/otel": "1.10.1",
        "axios": "1.7.9",
        "class-variance-authority": "0.7.1",
        "clsx": "2.1.1",
        "dayjs": "1.11.13",
        "framer-motion": "12.0.6",
        "isomorphic-dompurify": "2.20.0",
        "lodash-es": "4.17.21",
        "lucide-react": "0.474.0",
        "nanoid": "5.0.9",
        "next": "15.1.6",
        "prom-client": "15.1.3",
        "query-string": "9.1.1",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-hook-form": "7.54.2",
        "react-use": "17.6.0",
        "server-only": "0.0.1",
        "tailwind-merge": "2.6.0",
        "zod": "3.24.1"
    },
    "devDependencies": {
        "@million/lint": "1.0.14",
        "@playwright/test": "1.50.0",
        "@repo/ts-config": "*",
        "@statoscope/cli": "5.28.3",
        "@statoscope/stats-validator-plugin-webpack": "5.28.3",
        "@statoscope/webpack-plugin": "5.28.3",
        "@storybook/addon-designs": "8.0.4",
        "@storybook/addon-essentials": "8.5.2",
        "@storybook/addon-interactions": "8.5.2",
        "@storybook/addon-links": "8.5.2",
        "@storybook/addon-onboarding": "8.5.2",
        "@storybook/addon-storysource": "8.5.2",
        "@storybook/blocks": "8.5.2",
        "@storybook/nextjs": "8.5.2",
        "@storybook/react": "8.5.2",
        "@storybook/test": "8.5.2",
        "@tailwindcss/postcss": "4.0.0",
        "@testing-library/react": "16.2.0",
        "@testing-library/user-event": "14.6.1",
        "@total-typescript/ts-reset": "0.6.1",
        "@types/lodash-es": "4.17.12",
        "@types/node": "22.10.10",
        "@types/react": "19.0.8",
        "@types/react-dom": "19.0.3",
        "@typescript-eslint/utils": "8.21.0",
        "@vitejs/plugin-react": "4.3.4",
        "@vitest/browser": "3.0.4",
        "@vitest/coverage-v8": "3.0.4",
        "@webpractik/eslint-config-next": "2.0.0",
        "commitizen": "4.3.1",
        "cssnano": "7.0.6",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "9.19.0",
        "git-cz": "4.9.0",
        "husky": "9.1.7",
        "jiti": "2.4.2",
        "jsdom": "26.0.0",
        "kill-port": "2.0.1",
        "knip": "5.43.5",
        "lint-staged": "15.4.3",
        "minimatch": "10.0.1",
        "postcss": "8.5.1",
        "prettier": "3.4.2",
        "prettier-plugin-tailwindcss": "0.6.11",
        "rimraf": "6.0.1",
        "storybook": "8.5.2",
        "tailwindcss": "4.0.0",
        "type-fest": "4.33.0",
        "typescript": "5.7.3",
        "vite-tsconfig-paths": "5.1.4",
        "vitest": "3.0.4"
    },
    "engines": {
        "node": "^22",
        "npm": "^10"
    },
    "config": {
        "commitizen": {
            "path": "git-cz"
        }
    },
    "browserslist": [
        "defaults",
        "not dead"
    ],
    "workspaces": [
        "packages/*"
    ],
    "trustedDependencies": [
        "@sentry/cli",
        "sharp"
    ]
}
