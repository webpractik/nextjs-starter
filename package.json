{
  "name": "nextjs-starter",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "build": "next build",
    "dev": "kill-port 3000 && NODE_OPTIONS='--inspect' next dev -p 3000",
    "prod": "next start",
    "type-check": "tsc --project tsconfig.json --noEmit --pretty",
    "lint": "next lint --quiet --fix",
    "format": "pretty-quick --ignore-path .gitignore --pattern \"src/**/*.+(ts|js|tsx|json)\"",
    "analyze": "cross-env ANALYZE=true npm run build",
    "check-all": "run-p type-check lint format",
    "api-codegen": "openapi --input ./swagger.json --output ./src/api --request ./src/api/request.ts --useOptions",
    "storybook": "kill-port 6006 && start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "build-tokens": "style-dictionary build --config tokens.config.json"
  },
  "dependencies": {
    "@sentry/nextjs": "7.30.0",
    "@storybook/react": "6.5.15",
    "@tanstack/react-query": "4.22.0",
    "@tanstack/react-query-devtools": "4.22.0",
    "axios": "^1.2.2",
    "clsx": "^1.2.1",
    "form-data": "4.0.0",
    "lodash": "4.17.21",
    "mobx": "^6.7.0",
    "mobx-react-lite": "3.4.0",
    "modern-normalize": "1.1.0",
    "nanoid": "^4.0.0",
    "next": "13.1.2",
    "next-seo": "5.15.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-query-kit": "1.3.1",
    "react-use": "17.4.0",
    "sharp": "0.31.3"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@next/bundle-analyzer": "13.1.2",
    "@storybook/addon-actions": "6.5.15",
    "@storybook/addon-essentials": "6.5.15",
    "@storybook/addon-interactions": "6.5.15",
    "@storybook/addon-links": "6.5.15",
    "@storybook/builder-webpack5": "6.5.15",
    "@storybook/manager-webpack5": "6.5.15",
    "@storybook/testing-library": "0.0.13",
    "@types/lodash": "4.14.191",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "babel-loader": "9.1.2",
    "commitizen": "4.2.6",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "css-loader": "6.7.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.1.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "express": "4.18.2",
    "http-proxy-middleware": "2.0.6",
    "husky": "8.0.3",
    "kill-port": "^2.0.1",
    "lint-staged": "^13.1.0",
    "msw": "1.1.0",
    "msw-storybook-addon": "1.6.3",
    "npm-run-all": "4.1.5",
    "openapi-typescript-codegen": "0.23.0",
    "prettier": "^2.8.2",
    "pretty-quick": "3.1.3",
    "react-docgen-typescript": "2.2.2",
    "rimraf": "4.0.4",
    "sass": "^1.57.1",
    "sass-loader": "13.2.0",
    "storybook-addon-next": "1.7.1",
    "storybook-addon-next-router": "4.0.2",
    "style-dictionary": "3.7.2",
    "style-loader": "3.3.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">= 18",
    "npm": ">= 8"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not dead"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
