{
    "name": "nextjs-starter",
    "version": "7.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "next build",
        "dev": "kill-port 3000 && next dev -p 3000 --turbo",
        "prod": "next start",
        "check:ts": "tsc --project tsconfig.json --noEmit --pretty",
        "check:lint": "next lint",
        "check:format": "prettier . --write --cache --log-level=silent",
        "check:test": "vitest run",
        "check:e2e": "playwright test",
        "check:all": "npm run check:ts && npm run check:lint && npm run check:test && npm run check:format",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "analyze": "ANALYZE=true npm run build",
        "client:rsdoctor": "rsdoctor analyze --profile .next/.rsdoctor/manifest.json",
        "server:rsdoctor": "rsdoctor analyze --profile .next/server/.rsdoctor/manifest.json",
        "knip": "knip --strict",
        "jscpd": "npx jscpd",
        "clean": "rimraf .next",
        "storybook": "kill-port 6006 && storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "dependencies": {
        "@hookform/resolvers": "5.1.1",
        "@repo/api": "*",
        "@repo/core": "*",
        "@repo/design-tokens": "*",
        "@repo/logger": "*",
        "@repo/metrics": "*",
        "@repo/routes": "*",
        "@sentry/nextjs": "9.40.0",
        "@t3-oss/env-nextjs": "0.13.8",
        "@vercel/otel": "1.13.0",
        "class-variance-authority": "0.7.1",
        "clsx": "2.1.1",
        "dayjs": "1.11.13",
        "framer-motion": "12.23.6",
        "isomorphic-dompurify": "2.26.0",
        "lodash-es": "4.17.21",
        "lucide-react": "0.525.0",
        "nanoid": "5.1.5",
        "next": "15.4.2",
        "nuqs": "2.4.3",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-hook-form": "7.60.0",
        "react-use": "17.6.0",
        "server-only": "0.0.1",
        "tailwind-merge": "3.3.1",
        "tsafe": "1.8.5",
        "zod": "4.0.5"
    },
    "devDependencies": {
        "@playwright/test": "1.54.1",
        "@repo/ts-config": "*",
        "@rsdoctor/cli": "1.1.8",
        "@rsdoctor/webpack-plugin": "1.1.8",
        "@storybook/addon-designs": "10.0.2",
        "@storybook/addon-docs": "9.0.18",
        "@storybook/addon-links": "9.0.18",
        "@storybook/addon-onboarding": "9.0.18",
        "@storybook/nextjs": "9.0.18",
        "@tailwindcss/postcss": "4.1.11",
        "@testing-library/react": "16.3.0",
        "@testing-library/user-event": "14.6.1",
        "@total-typescript/ts-reset": "0.6.1",
        "@types/lodash-es": "4.17.12",
        "@types/node": "24.1.0",
        "@types/react": "19.1.8",
        "@types/react-dom": "19.1.6",
        "@typescript-eslint/utils": "8.38.0",
        "@vitejs/plugin-react": "4.7.0",
        "@vitest/browser": "3.2.4",
        "@vitest/coverage-v8": "3.2.4",
        "@webpractik/eslint-config-next": "2.0.3",
        "ajv": "8.17.1",
        "commitizen": "4.3.1",
        "cssnano": "7.1.0",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "9.31.0",
        "jiti": "2.4.2",
        "jsdom": "26.1.0",
        "kill-port": "2.0.1",
        "knip": "5.62.0",
        "lefthook": "1.12.2",
        "lint-staged": "16.1.2",
        "minimatch": "10.0.3",
        "postcss": "8.5.6",
        "prettier": "3.6.2",
        "prettier-plugin-tailwindcss": "0.6.14",
        "rimraf": "6.0.1",
        "storybook": "9.0.18",
        "tailwindcss": "4.1.11",
        "type-fest": "4.41.0",
        "typed-query-selector": "2.12.0",
        "typescript": "5.8.3",
        "vite-tsconfig-paths": "5.1.4",
        "vitest": "3.2.4"
    },
    "engines": {
        "node": "^22",
        "npm": "^10"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "browserslist": [
        "defaults",
        "not dead"
    ],
    "workspaces": [
        "packages/*"
    ],
    "trustedDependencies": [
        "@sentry/cli",
        "sharp"
    ],
    "overrides": {
        "storybook": "$storybook"
    }
}
