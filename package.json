{
    "name": "nextjs-starter",
    "version": "7.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "next build",
        "dev": "next dev --turbo",
        "prod": "node server.js || node .next/standalone/server.js",
        "check:ts": "tsc --project tsconfig.json --noEmit --pretty",
        "check:lint": "eslint",
        "check:lint-fix": "eslint --fix",
        "check:format": "prettier . --write --cache --log-level=silent",
        "check:test": "vitest run",
        "check:e2e": "playwright test",
        "check:all": "npm run check:ts && npm run check:lint && npm run check:test && npm run check:format",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "analyze": "ANALYZE=true npm run build",
        "client:rsdoctor": "rsdoctor analyze --profile .next/.rsdoctor/manifest.json",
        "server:rsdoctor": "rsdoctor analyze --profile .next/server/.rsdoctor/manifest.json",
        "knip": "knip --strict",
        "jscpd": "npx jscpd",
        "clean": "rimraf .next",
        "storybook": "storybook dev",
        "build-storybook": "storybook build"
    },
    "dependencies": {
        "@hookform/resolvers": "5.2.2",
        "@repo/api": "*",
        "@repo/core": "*",
        "@repo/design-tokens": "*",
        "@repo/logger": "*",
        "@repo/metrics": "*",
        "@sentry/nextjs": "10.34.0",
        "@t3-oss/env-nextjs": "0.13.10",
        "@vercel/otel": "2.1.0",
        "class-variance-authority": "0.7.1",
        "clsx": "2.1.1",
        "dayjs": "1.11.19",
        "framer-motion": "12.26.2",
        "isomorphic-dompurify": "2.35.0",
        "lodash-es": "4.17.22",
        "lucide-react": "0.562.0",
        "nanoid": "5.1.6",
        "next": "16.1.2",
        "nuqs": "2.8.6",
        "react": "19.2.3",
        "react-dom": "19.2.3",
        "react-hook-form": "7.71.1",
        "react-use": "17.6.0",
        "server-only": "0.0.1",
        "tailwind-merge": "3.4.0",
        "tsafe": "1.8.12",
        "zod": "4.3.5"
    },
    "devDependencies": {
        "@antfu/eslint-config": "7.0.1",
        "@playwright/test": "1.57.0",
        "@repo/ts-config": "*",
        "@rsdoctor/cli": "1.5.0",
        "@rsdoctor/webpack-plugin": "1.5.0",
        "@storybook/addon-designs": "11.1.1",
        "@storybook/addon-docs": "10.1.11",
        "@storybook/addon-links": "10.1.11",
        "@storybook/addon-onboarding": "10.1.11",
        "@storybook/nextjs": "10.1.11",
        "@tailwindcss/postcss": "4.1.18",
        "@tailwindcss/typography": "0.5.19",
        "@testing-library/react": "16.3.1",
        "@testing-library/user-event": "14.6.1",
        "@total-typescript/ts-reset": "0.6.1",
        "@types/lodash-es": "4.17.12",
        "@types/node": "25.0.9",
        "@types/react": "19.2.8",
        "@types/react-dom": "19.2.3",
        "@typescript-eslint/utils": "8.53.0",
        "@vitejs/plugin-react": "5.1.2",
        "@vitest/browser": "4.0.17",
        "@vitest/browser-playwright": "4.0.17",
        "@vitest/coverage-v8": "4.0.17",
        "@webpractik/eslint-config-next": "2.0.6",
        "ajv": "8.17.1",
        "babel-plugin-react-compiler": "1.0.0",
        "commitizen": "4.3.1",
        "cssnano": "7.1.2",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "9.39.2",
        "eslint-plugin-better-tailwindcss": "4.0.1",
        "eslint-plugin-format": "1.3.1",
        "eslint-plugin-react-hooks": "7.0.1",
        "eslint-plugin-react-refresh": "0.4.26",
        "jiti": "2.6.1",
        "jsdom": "27.4.0",
        "kill-port": "2.0.1",
        "knip": "5.81.0",
        "lefthook": "2.0.15",
        "lint-staged": "16.2.7",
        "postcss": "8.5.6",
        "prettier": "3.8.0",
        "rimraf": "6.1.2",
        "storybook": "10.1.11",
        "tailwindcss": "4.1.18",
        "tw-animate-css": "1.4.0",
        "type-fest": "5.4.1",
        "typed-query-selector": "2.12.0",
        "typescript": "5.9.3",
        "vite-tsconfig-paths": "6.0.4",
        "vitest": "4.0.17"
    },
    "engines": {
        "node": "^24",
        "npm": "^11"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "browserslist": [
        "defaults",
        "not dead"
    ],
    "workspaces": [
        "packages/*"
    ],
    "trustedDependencies": [
        "@sentry/cli",
        "sharp"
    ],
    "overrides": {
        "storybook": "$storybook",
        "eslint-plugin-react-hooks>zod-validation-error": "^4.0.0"
    }
}
