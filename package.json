{
    "name": "nextjs-starter",
    "version": "3.1.0",
    "private": true,
    "scripts": {
        "prepare": "husky install",
        "build": "next build",
        "dev": "npm run clean && kill-port 3000 && next dev -p 3000",
        "prod": "next start",
        "type-check": "tsc --project tsconfig.json --noEmit --pretty",
        "lint": "next lint --format=pretty",
        "stylelint": "stylelint '**/*.module.sass' --fix --config ./config/stylelint.config.mjs",
        "format": "pretty-quick --ignore-path .gitignore --pattern \"src/**/*.+(ts|js|tsx|json)\"",
        "analyze": "cross-env ANALYZE=true npm run build",
        "check-all": "run-p type-check lint format test-run",
        "clean": "rimraf .next",
        "api-codegen": "openapi --input ./swagger.json --output ./src/api --request ./src/api/request.ts --useOptions",
        "storybook": "kill-port 6006 && storybook dev -p 6006 --quiet",
        "build-storybook": "storybook build",
        "build-tokens": "style-dictionary build --config ./config/tokens.config.json",
        "test": "vitest",
        "test-run": "vitest run",
        "coverage": "vitest run --coverage",
        "type-coverage": "npx typescript-coverage-report",
        "cruiser": "depcruise src --include-only \"^src/app\" --config ./config/.dependency-cruiser.js --output-type dot | dot -T svg -Grankdir=TD | tee dependency-graph.svg | depcruise-wrap-stream-in-html > ./docs.html",
        "gen": "generate-react component"
    },
    "dependencies": {
        "@sentry/nextjs": "7.57.0",
        "@t3-oss/env-nextjs": "0.6.0",
        "@tanstack/react-query": "4.29.19",
        "@tanstack/react-query-devtools": "4.29.19",
        "axios": "1.4.0",
        "clsx": "1.2.1",
        "dompurify": "3.0.4",
        "form-data": "4.0.0",
        "lodash": "4.17.21",
        "mobx": "6.9.0",
        "mobx-react-lite": "3.4.3",
        "modern-normalize": "2.0.0",
        "nanoid": "4.0.2",
        "next": "13.4.8",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-query-kit": "1.4.5",
        "react-use": "17.4.0",
        "sharp": "0.32.1",
        "zod": "3.21.4"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "13.4.8",
        "@storybook/addon-essentials": "7.0.26",
        "@storybook/addon-interactions": "7.0.26",
        "@storybook/addon-links": "7.0.26",
        "@storybook/nextjs": "7.0.26",
        "@storybook/react": "7.0.26",
        "@testing-library/react": "14.0.0",
        "@testing-library/user-event": "14.4.3",
        "@total-typescript/ts-reset": "0.4.2",
        "@types/dompurify": "3.0.2",
        "@types/lodash": "4.14.195",
        "@types/node": "20.4.0",
        "@types/react": "18.2.14",
        "@types/react-dom": "18.2.6",
        "@typescript-eslint/eslint-plugin": "5.61.0",
        "@typescript-eslint/parser": "5.61.0",
        "@vitejs/plugin-react": "4.0.1",
        "@vitest/coverage-c8": "0.32.4",
        "commitizen": "4.3.0",
        "cors": "2.8.5",
        "cross-env": "7.0.3",
        "css-loader": "6.8.1",
        "cz-conventional-changelog": "3.3.0",
        "dependency-cruiser": "13.1.3",
        "eslint": "8.44.0",
        "eslint-config-next": "13.4.8",
        "eslint-config-webpractik": "github:webpractik/eslint-config-webpractik",
        "eslint-formatter-pretty": "5.0.0",
        "generate-react-cli": "8.3.0",
        "husky": "8.0.3",
        "jsdom": "22.1.0",
        "kill-port": "2.0.1",
        "lint-staged": "13.2.3",
        "npm-run-all": "4.1.5",
        "openapi-typescript-codegen": "0.25.0",
        "postcss-sass": "0.5.0",
        "prettier": "2.8.8",
        "pretty-quick": "3.1.3",
        "react-docgen-typescript": "2.2.2",
        "rimraf": "5.0.1",
        "sass": "1.63.6",
        "sass-loader": "13.3.2",
        "storybook": "7.0.26",
        "storybook-dark-mode": "3.0.0",
        "style-dictionary": "3.8.0",
        "style-loader": "3.3.3",
        "stylelint": "15.10.1",
        "stylelint-config-css-modules": "4.2.0",
        "stylelint-config-idiomatic-order": "9.0.0",
        "stylelint-config-sass-guidelines": "10.0.0",
        "stylelint-config-standard-scss": "10.0.0",
        "typescript": "5.1.6",
        "vite-tsconfig-paths": "4.2.0",
        "vitest": "0.32.4"
    },
    "engines": {
        "node": "^18",
        "npm": "^9"
    },
    "browserslist": {
        "development": [
            "last 2 chrome versions",
            "last 2 firefox versions"
        ],
        "production": [
            ">1%",
            "last 4 versions",
            "Firefox ESR",
            "not ie < 11"
        ]
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    }
}
