{
    "name": "nextjs-starter",
    "version": "7.0.0",
    "private": true,
    "scripts": {
        "prepare": "node .husky/install.mjs",
        "build": "next build",
        "dev": "kill-port 3000 && next dev -p 3000",
        "prod": "next start",
        "check:ts": "tsc --project tsconfig.json --noEmit --pretty",
        "check:lint": "next lint",
        "check:format": "prettier . --write --cache --log-level=silent",
        "check:test": "vitest --config tests/vitest.config.mts run",
        "check:e2e": "playwright test",
        "check:all": "npm run check:ts && npm run check:lint && npm run check:test && npm run check:format",
        "test:watch": "vitest --config tests/vitest.config.mts",
        "test:coverage": "vitest --config tests/vitest.config.mts run --coverage",
        "analyze": "ANALYZE=true npm run build && statoscope validate --input .next/stats.json",
        "million": "MILLION=true npm run dev",
        "knip": "knip --strict",
        "jscpd": "npx jscpd",
        "clean": "rimraf .next",
        "dr:build": "npx declarative-routing build",
        "dr:build:watch": "npx declarative-routing build --watch",
        "storybook": "kill-port 6006 && storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "dependencies": {
        "@hookform/resolvers": "3.10.0",
        "@repo/api": "*",
        "@repo/core": "*",
        "@repo/design-tokens": "*",
        "@sentry/nextjs": "8.48.0",
        "@t3-oss/env-nextjs": "0.11.1",
        "@tanstack/react-query": "5.64.0",
        "@tanstack/react-query-devtools": "5.64.0",
        "@tanstack/react-query-next-experimental": "5.64.0",
        "@vercel/otel": "1.10.0",
        "axios": "1.7.9",
        "class-variance-authority": "0.7.1",
        "clsx": "2.1.1",
        "dayjs": "1.11.13",
        "framer-motion": "11.17.0",
        "isomorphic-dompurify": "2.20.0",
        "lodash-es": "4.17.21",
        "lucide-react": "0.471.0",
        "nanoid": "5.0.9",
        "next": "15.1.4",
        "prom-client": "15.1.3",
        "query-string": "9.1.1",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-hook-form": "7.54.2",
        "react-use": "17.6.0",
        "server-only": "0.0.1",
        "tailwind-merge": "2.6.0",
        "tailwindcss-animate": "1.0.7",
        "zod": "3.24.1"
    },
    "devDependencies": {
        "@million/lint": "1.0.14",
        "@playwright/test": "1.49.1",
        "@repo/tailwind-config": "*",
        "@repo/ts-config": "*",
        "@statoscope/cli": "5.28.3",
        "@statoscope/stats-validator-plugin-webpack": "5.28.3",
        "@statoscope/webpack-plugin": "5.28.3",
        "@storybook/addon-designs": "8.0.4",
        "@storybook/addon-essentials": "8.4.7",
        "@storybook/addon-interactions": "8.4.7",
        "@storybook/addon-links": "8.4.7",
        "@storybook/addon-onboarding": "8.4.7",
        "@storybook/addon-storysource": "8.4.7",
        "@storybook/blocks": "8.4.7",
        "@storybook/nextjs": "8.4.7",
        "@storybook/react": "8.4.7",
        "@storybook/test": "8.4.7",
        "@testing-library/jest-dom": "6.6.3",
        "@testing-library/react": "16.1.0",
        "@testing-library/user-event": "14.5.2",
        "@total-typescript/ts-reset": "0.6.1",
        "@types/lodash-es": "4.17.12",
        "@types/node": "22.10.5",
        "@types/react": "19.0.5",
        "@types/react-dom": "19.0.3",
        "@types/webpack-bundle-analyzer": "4.7.0",
        "@typescript-eslint/eslint-plugin": "7.18.0",
        "@typescript-eslint/parser": "7.18.0",
        "@vitejs/plugin-react": "4.3.4",
        "@vitest/coverage-v8": "2.1.8",
        "autoprefixer": "10.4.20",
        "commitizen": "4.3.1",
        "cssnano": "7.0.6",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "8.57.0",
        "eslint-config-custom": "*",
        "git-cz": "4.9.0",
        "husky": "9.1.7",
        "jiti": "2.4.0",
        "jsdom": "26.0.0",
        "kill-port": "2.0.1",
        "knip": "5.42.0",
        "lint-staged": "15.3.0",
        "minimatch": "10.0.1",
        "postcss": "8.4.49",
        "prettier": "3.4.2",
        "rimraf": "6.0.1",
        "storybook": "8.4.7",
        "tailwindcss": "3.4.17",
        "type-fest": "4.32.0",
        "typescript": "5.7",
        "vite-tsconfig-paths": "5.1.4",
        "vitest": "2.1.8",
        "vitest-fail-on-console": "0.7.1"
    },
    "engines": {
        "node": "^22",
        "npm": "^10"
    },
    "config": {
        "commitizen": {
            "path": "git-cz"
        }
    },
    "browserslist": [
        "defaults",
        "not dead"
    ],
    "workspaces": [
        "packages/*"
    ],
    "trustedDependencies": [
        "@sentry/cli",
        "core-js-pure",
        "es5-ext",
        "esbuild",
        "sharp"
    ]
}
