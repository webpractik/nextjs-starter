{
    "name": "nextjs-starter",
    "version": "6.0.0",
    "private": true,
    "scripts": {
        "prepare": "node .husky/install.mjs",
        "build": "next build",
        "dev": "kill-port 3000 && next dev -p 3000",
        "prod": "next start",
        "check:ts": "tsc --project tsconfig.json --noEmit --pretty",
        "check:lint": "next lint",
        "check:format": "prettier . --write --cache --log-level=silent",
        "check:test": "vitest --config ./tests/vitest.config.mts run",
        "check:e2e": "playwright test",
        "check:all": "npm run check:ts && npm run check:lint && npm run check:test && npm run check:format",
        "test:watch": "vitest --config ./tests/vitest.config.mts",
        "test:coverage": "vitest --config ./tests/vitest.config.mts run --coverage",
        "analyze": "ANALYZE=true npm run build",
        "million": "MILLION=true npm run dev",
        "clean": "rimraf .next",
        "dr:build": "npx declarative-routing build",
        "dr:build:watch": "npx declarative-routing build --watch",
        "storybook": "kill-port 6006 && storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "dependencies": {
        "@hookform/resolvers": "3.9.0",
        "@repo/api": "*",
        "@repo/core": "*",
        "@repo/design-tokens": "*",
        "@sentry/nextjs": "8.35.0",
        "@t3-oss/env-nextjs": "0.11.1",
        "@tanstack/react-query": "5.59.15",
        "@tanstack/react-query-devtools": "5.59.15",
        "@tanstack/react-query-next-experimental": "5.59.15",
        "@vercel/otel": "1.10.0",
        "axios": "1.7.7",
        "class-variance-authority": "0.7.0",
        "clsx": "2.1.1",
        "dayjs": "1.11.13",
        "framer-motion": "11.11.9",
        "isomorphic-dompurify": "2.16.0",
        "lodash-es": "4.17.21",
        "lucide-react": "0.453.0",
        "nanoid": "5.0.7",
        "next": "15.0.0",
        "prom-client": "15.1.3",
        "query-string": "9.1.1",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-hook-form": "7.53.1",
        "react-use": "17.5.1",
        "server-only": "0.0.1",
        "tailwind-merge": "2.5.4",
        "tailwindcss-animate": "1.0.7",
        "zod": "3.23.8"
    },
    "devDependencies": {
        "@million/lint": "1.0.11",
        "@playwright/test": "1.48.1",
        "@repo/tailwind-config": "*",
        "@repo/ts-config": "*",
        "@storybook/addon-designs": "8.0.3",
        "@storybook/addon-essentials": "8.3.6",
        "@storybook/addon-interactions": "8.3.6",
        "@storybook/addon-links": "8.3.6",
        "@storybook/addon-onboarding": "8.3.6",
        "@storybook/addon-storysource": "8.3.6",
        "@storybook/blocks": "8.3.6",
        "@storybook/nextjs": "8.3.6",
        "@storybook/react": "8.3.6",
        "@storybook/test": "8.3.6",
        "@testing-library/jest-dom": "6.6.2",
        "@testing-library/react": "16.0.1",
        "@testing-library/user-event": "14.5.2",
        "@total-typescript/ts-reset": "0.6.1",
        "@types/lodash-es": "4.17.12",
        "@types/node": "22.7.8",
        "@types/react": "18.3.11",
        "@types/react-dom": "18.3.1",
        "@types/webpack-bundle-analyzer": "4.7.0",
        "@typescript-eslint/eslint-plugin": "7.18.0",
        "@typescript-eslint/parser": "7.18.0",
        "@vitejs/plugin-react": "4.3.3",
        "@vitest/coverage-v8": "2.1.3",
        "autoprefixer": "10.4.20",
        "commitizen": "4.3.1",
        "cssnano": "7.0.6",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "8.57.0",
        "eslint-config-custom": "*",
        "git-cz": "4.9.0",
        "husky": "9.1.6",
        "jiti": "2.3.3",
        "jsdom": "25.0.1",
        "kill-port": "2.0.1",
        "lint-staged": "15.2.10",
        "minimatch": "10.0.1",
        "postcss": "8.4.47",
        "prettier": "3.3.3",
        "rimraf": "6.0.1",
        "storybook": "8.3.6",
        "tailwindcss": "3.4.14",
        "type-fest": "4.26.1",
        "typescript": "5.6",
        "vite-tsconfig-paths": "5.0.1",
        "vitest": "2.1.3",
        "vitest-fail-on-console": "0.7.1",
        "webpack-bundle-analyzer": "4.10.2"
    },
    "engines": {
        "node": "^20",
        "npm": "^10"
    },
    "config": {
        "commitizen": {
            "path": "git-cz"
        }
    },
    "browserslist": [
        "defaults",
        "not dead"
    ],
    "workspaces": [
        "packages/*"
    ],
    "trustedDependencies": [
        "@sentry/cli",
        "core-js-pure",
        "es5-ext",
        "esbuild",
        "sharp"
    ]
}
